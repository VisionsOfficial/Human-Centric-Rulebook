    <%- include('partials/head') %>
    <title>Contract as Text</title>
</head>
<body>
    <%- include('partials/header') %>

    <div class="container">


        <h4>Data Provider</h4>

        <p><b class="text-primary"><%= contract.serviceExport.name %></b> acts as the Data Provider</p>


        <h4>Schedules</h4>

        <table class="table table-striped">
            <thead>
                <th>Schedule</th>
                <th>Description</th>
            </thead>
            <tbody>
                <tr>
                    <td>1</td>
                    <td>
                        <% contract.dataSharing.forEach(item => { %>
                            <ul>
                                <% item.datatypes.forEach(datatype => { %>
                                    <li><b class="text-primary"><%= datatype.name %></b></li>
                                    <li><b class="text-primary"><%= datatype.description %></b></li>
                                    <hr>
                                <% }) %>
                            </ul>
                        <% }) %>
                    </td>
                </tr>
            </tbody>
        </table>




